<div class="row">
  <div class="col-md-12">
    <div class="box">
      <div class="box-header">
        <ul class="nav nav-tabs nav-tabs-left">
          <li class="active"><a href="#home" data-toggle="tab"><i class="icon-home"></i></a></li>
        </ul>
        <div class="title"><%= "MODEL" %></div>
      </div>

      <div class="box-content">
        <div class="tab-content">
          <div class="tab-pane active" id="home">
            <ul class="box-list">
                              <li>
                  <span><strong><%= t'.name' %></strong> <%= @client.name %></span>
                </li>
                              <li>
                  <span><strong><%= t'.contact' %></strong> <%= @client.contact %></span>
                </li>
                              <li>
                  <span><strong><%= t'.email1' %></strong> <%= @client.email1 %></span>
                </li>
                              <li>
                  <span><strong><%= t'.email2' %></strong> <%= @client.email2 %></span>
                </li>
                              <li>
                  <span><strong><%= t'.webpage' %></strong> <%= @client.webpage %></span>
                </li>
                              <li>
                  <span><strong><%= t'.phone1' %></strong> <%= @client.phone1 %></span>
                </li>
                              <li>
                  <span><strong><%= t'.phone2' %></strong> <%= @client.phone2 %></span>
                </li>
                              <li>
                  <span><strong><%= t'.fax' %></strong> <%= @client.fax %></span>
                </li>
                              <li>
                  <span><strong><%= t'.address1' %></strong> <%= @client.address1 %></span>
                </li>
                              <li>
                  <span><strong><%= t'.address2' %></strong> <%= @client.address2 %></span>
                </li>
                              <li>
                  <span><strong><%= t'.city' %></strong> <%= @client.city %></span>
                </li>
                              <li>
                  <span><strong><%= t'.state' %></strong> <%= @client.state %></span>
                </li>
                              <li>
                  <span><strong><%= t'.zip' %></strong> <%= @client.zip %></span>
                </li>
                              <li>
                  <span><strong><%= t'.country' %></strong> <%= @client.country %></span>
                </li>
                              <li>
                  <span><strong><%= t'.description' %></strong> <%= @client.description %></span>
                </li>
                              <li>
                  <span><strong><%= t'.company_id' %></strong> <%= @client.company %></span>
                </li>
                              <li>
                  <span><strong><%= t'.branch_id' %></strong> <%= @client.branch %></span>
                </li>
                              <li>
                  <span><strong><%= t'.latitude' %></strong> <%= @client.latitude %></span>
                </li>
                              <li>
                  <span><strong><%= t'.longitude' %></strong> <%= @client.longitude %></span>
                </li>
                          </ul>
          </div>
        </div>
      </div>
      <div class="box-footer padded">
        <span class="pull-right">
          <%= render "layouts/action_links" %>
        </span>
      </div>
    </div>
  </div>
</div>
